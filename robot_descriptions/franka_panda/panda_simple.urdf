<?xml version="1.0" ?>
<!-- Simplified Franka Panda with tool mount - uses primitive shapes instead of meshes -->
<robot name="panda">
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="grey">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  <material name="orange">
    <color rgba="1 0.5 0 1"/>
  </material>

  <link name="panda_link0">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <mass value="2.9"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.025"/>
      <geometry><cylinder radius="0.06" length="0.05"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.025"/>
      <geometry><cylinder radius="0.06" length="0.05"/></geometry>
    </collision>
  </link>

  <link name="panda_link1">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.07"/>
      <mass value="2.7"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.07"/>
      <geometry><cylinder radius="0.05" length="0.14"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.07"/>
      <geometry><cylinder radius="0.05" length="0.14"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.333"/>
    <parent link="panda_link0"/>
    <child link="panda_link1"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-2.9671" upper="2.9671" velocity="2.1750"/>
  </joint>

  <link name="panda_link2">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.03 0"/>
      <mass value="2.7"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.05" length="0.14"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.05" length="0.14"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint2" type="revolute">
    <origin rpy="-1.5708 0 0" xyz="0 0 0"/>
    <parent link="panda_link1"/>
    <child link="panda_link2"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-1.8326" upper="1.8326" velocity="2.1750"/>
  </joint>

  <link name="panda_link3">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.12"/>
      <mass value="2.4"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12"/>
      <geometry><cylinder radius="0.045" length="0.24"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.12"/>
      <geometry><cylinder radius="0.045" length="0.24"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint3" type="revolute">
    <origin rpy="1.5708 0 0" xyz="0 -0.316 0"/>
    <parent link="panda_link2"/>
    <child link="panda_link3"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-2.9671" upper="2.9671" velocity="2.1750"/>
  </joint>

  <link name="panda_link4">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.03 0"/>
      <mass value="2.4"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.045" length="0.14"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.045" length="0.14"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint4" type="revolute">
    <origin rpy="1.5708 0 0" xyz="0.0825 0 0"/>
    <parent link="panda_link3"/>
    <child link="panda_link4"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-3.1416" upper="0.0873" velocity="2.1750"/>
  </joint>

  <link name="panda_link5">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.13"/>
      <mass value="2.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.13"/>
      <geometry><cylinder radius="0.04" length="0.26"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.13"/>
      <geometry><cylinder radius="0.04" length="0.26"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint5" type="revolute">
    <origin rpy="-1.5708 0 0" xyz="-0.0825 0.384 0"/>
    <parent link="panda_link4"/>
    <child link="panda_link5"/>
    <axis xyz="0 0 1"/>
    <limit effort="12" lower="-2.9671" upper="2.9671" velocity="2.6100"/>
  </joint>

  <link name="panda_link6">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.6"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.04" length="0.1"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.04" length="0.1"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint6" type="revolute">
    <origin rpy="1.5708 0 0" xyz="0 0 0"/>
    <parent link="panda_link5"/>
    <child link="panda_link6"/>
    <axis xyz="0 0 1"/>
    <limit effort="12" lower="-0.0175" upper="3.7525" velocity="2.6100"/>
  </joint>

  <link name="panda_link7">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <mass value="0.7"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.04"/>
      <geometry><cylinder radius="0.035" length="0.08"/></geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.04"/>
      <geometry><cylinder radius="0.035" length="0.08"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint7" type="revolute">
    <origin rpy="1.5708 0 0" xyz="0.088 0 0"/>
    <parent link="panda_link6"/>
    <child link="panda_link7"/>
    <axis xyz="0 0 1"/>
    <limit effort="12" lower="-2.9671" upper="2.9671" velocity="2.6100"/>
  </joint>

  <!-- Flange (end-effector mounting plate) -->
  <link name="panda_link8">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.03" length="0.02"/></geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.03" length="0.02"/></geometry>
    </collision>
  </link>

  <joint name="panda_joint8" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.107"/>
    <parent link="panda_link7"/>
    <child link="panda_link8"/>
  </joint>

  <!-- Tool mount - this is where generated tools attach -->
  <link name="tool_mount">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <geometry><sphere radius="0.015"/></geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <geometry><sphere radius="0.015"/></geometry>
    </collision>
  </link>

  <joint name="tool_mount_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.02"/>
    <parent link="panda_link8"/>
    <child link="tool_mount"/>
  </joint>

</robot>
